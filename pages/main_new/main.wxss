  display: flex;
  flex-direction: row;
  width: 100%;
  border-bottom: 1rpx solid #eeeeee;
  vertical-align: center;
  padding: 0rpx 10rpx;
  margin: 0;
  background-color: #ffffff;

  z-index: 5;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.con-date {
  color: #757575;
  margin: 10rpx;
  padding-left: 10rpx;
  font-size: 80%;
}
.con-info {
  margin: 10rpx;
  text-align: left;
  width: 74%;
  display: flex;
  flex-direction: row;
}

.empty_wish {
  color: #757575;
  margin: 50rpx;
  text-align: center;
  font-size: 90%;
}

.btnblock {
  width: 100%;
  border-radius: 30rpx;
  background: #e60c12;
  border: solid 1rpx #6a3906;
  color: #ffffff;
}
.addwishbtn {
  color: #f08080;
  margin: 30rpx auto;
  width: 60%;
  border: 3rpx solid #f08080;
  border-radius: 20rpx;
}

.con-trimg {
  width: 60rpx;
  height: 60rpx;
  margin: 5rpx 10rpx;
}
.con-enter {
  align-items: center;
  justify-content: center;
  color: #757575;
}

.del {
  border: 2rpx solid #f08080;
  border-radius: 20rpx;
  margin: 18rpx 10rpx;
  padding: 6rpx 30rpx;
  font-size: 86%;
  align-items: center;
  justify-content: center;
  vertical-align: center;
  color: #f08080;
}

.touch-move-active {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.timeline {
  width: 98%;
  margin: 30rpx auto;
  padding: 10rpx 10rpx 130rpx;
  padding-left: 30rpx;
  text-align: center;
  flex: 1;
  overflow: hidden;
  height: auto;
  box-sizing: border-box;
}

.timeline-line {
  margin-left: 20px;
  border-left: 1px solid #6a3906;
  height: 5px;
  background: #fff;
}

.timeline-line-up {
  margin-left: 20px;
  border-left: 1px solid #6a3906;
  height: 10px;
  background: #fff;
}

.timeline-circle {
  margin-left: 15px;
  border: 5px solid #6a3906;
  border-radius: 5px;
  width: 2px;
  height: 2px;
  border-color: #6a3906;
  background-color: #6a3906;
}

.timeline-line-down {
  margin-left: 20px;
  border-left: 1px solid #6a3906;
  height: 20px;
  background: #fff;
}

.timeline-timestamp {
  border: 2px solid #6a3906;
  border-radius: 20rpx;
  width: 80px;
  height: 20px;
  padding: 5rpx 10rpx;
  border-color: #6a3906;
  color: #6a3906;
}

.timeline-asof {
  font-size: 14px;
}

.timeline-btn {
  /* font-size: 70%;
  margin: 10rpx 5rpx;
  padding: 1rpx 18rpx;
  border-radius: 15rpx;
  border: solid 4rpx #6a3906;
  color: #6a3906; */
  background-color: none;
}

.timeline-entry {
  display: flex;
  flex-direction: row;
  align-items: top;
  background-color: #ffffff;
  width: 94%;
  margin: 0rpx;
}

.timeline-left {
  width: 30%;
  text-align: left;
  display: flex;
  flex-direction: row;
}
.timeline-middle {
  margin-top: 2rpx;
  padding-right: 30rpx;
  width: 52%;
  color: #6a3906;
  font-size: 80%;
  text-align: left;
}
.timeline-right {
  width: 18%;
  display: flex;
  align-items: center;
}

.time-point-left {
  width: 58%;
}

.time-point-right {
  width: 40%;
  color: #6a3906;
  font-weight: bold;
  text-align: center;
}

.buttonView {
  /* width: 750rpx; */
  margin: auto;
  position: fixed;
  bottom: 0;
  background: linear-gradient(
    to top,
    rgba(255, 255, 255, 1) 0%,
    rgba(255, 255, 255, 0.76) 50%,
    rgba(255, 255, 255, 0) 100%
  );
  left: 0;
  right: 0;
  padding: 25rpx 50rpx;
  box-sizing: border-box;
  text-align: center;
}
.main_btn {
  width: 562rpx;
  margin: auto;
  border-radius: 30rpx;
  background: #e60c12;
  border: solid 5rpx #6a3906;
  color: #ffffff;
}
.timeline-middle view {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 360rpx;
  font-size: 30rpx;
}
.icon-see {
  width: 130rpx;
  height: 50rpx;
  vertical-align: middle;
}
.friend-timeline .timeline-entry {
  height: 160rpx;
}
.friend-timeline .time-point-right {
  line-height: 160rpx;
  font-family: TrebuchetMS, Rotobo, "Microsoft YaHei", sans-serif;
}
.friend-timeline .timeline-line-up {
  height: 70rpx;
}
.friend-timeline .timeline-line-down {
  height: 70rpx;
}
.friend-timeline .description {
  font-size: 25rpx;
  background: #ffd45a;
  display: inline-block;
  padding: 3rpx 14rpx;
  border-radius: 10rpx;
  border: 2rpx solid #e8b90b;
  width: auto;
  position: relative;
  margin-bottom: 15rpx;
}
.friend-timeline .description-text {
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 330rpx;
  display: inline-block;
  max-width: 330rpx;
  overflow: hidden;
  line-height: 25rpx;
}
.friend-timeline .description view.bot,
.friend-timeline .description view.top {
  width: 0;
  height: 0;
  font-size: 0;
  overflow: hidden;
  position: absolute;
  z-index: 1;
  left: 14rpx;
}
.friend-timeline .description view.bot {
  border-width: 10rpx;
  border-style: solid dashed dashed;
  border-color: #e8b90b transparent transparent;
  bottom: -20rpx;
}
.friend-timeline .timeline-middle view {
  overflow: visible;
  width: auto;
}
.friend-timeline .description view.top {
  border-width: 10rpx;
  border-style: solid dashed dashed;
  border-color: #ffd45a transparent transparent;
  bottom: -18rpx;
}
.friend-timeline .timeline-right {
  display: inline-block;
}
.friend-timeline .icon-see {
  margin-top: 20rpx;
}

.friend-timeline .timeline-middle {
  padding-top: 25rpx;
}

.wish-save-btn {
  margin-top: 40rpx;
  width: 562rpx;
  height: 100rpx;
  padding: 0;
}
.wish-share-btn {
  margin: auto;
  width: 562rpx;
  height: 100rpx;
}
.mini-time {
  font-size: 20rpx;
  font-weight: normal;
}
