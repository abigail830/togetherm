/**index.wxss**/
.container {
  padding: 0rpx;
  margin: 0rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.fullwidth {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #ffffff;
  width: 100%;
  padding: 0;
  margin: 0rpx;
  text-align: center;
}

.userinfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #ffffff;
  width: 90%;
  padding: 10rpx;
  margin: 0rpx;
}
.left {
  width: 30%;
  padding: 10rpx;
  text-align: right;
}
.middle {
  width: 30%;
  color: #6a3906;
  font-size: 80%;
  text-align: left;
  padding-top: 60rpx;
}
.right {
  width: 30%;
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo {
  width: 127.96rpx;
  height: 34.12rpx;
  margin: 15rpx;
  margin-top: 5rpx;
}
.help {
  font-size: 70%;
  padding: 3rpx;
  height: 50rpx;
  line-height: 40rpx;
  width: 140rpx;
  border-radius: 15rpx;
  border: solid 4rpx #6a3906;
  color: #6a3906;
  background-color: #94ddf8;
}

.userinfo-avatar {
  position: absolute;
  top: 42rpx;
  left: 84rpx;
  margin: 10rpx;
  width: 120rpx;
  height: 120rpx;
  overflow: hidden;
  border-radius: 50%;
  background-color: #94ddf8;
}
.icon_bg {
  position: absolute;
  top: 40rpx;
  left: 80rpx;
  width: 150rpx;
  height: 150rpx;
}
.icon_out {
  height: 200rpx;
}

.countinfo {
  display: flex;
  flex-direction: row;
  width: 94%;
  margin: 0rpx;
  text-align: center;
  background-color: #ffffff;
  color: #353535;
}

.countcell {
  display: flex;
  flex-direction: column;
  width: 42%;
  padding: 8rpx;
  margin: 10rpx;
  border-radius: 30rpx;
  border: solid 10rpx #6a3906;
  margin-top: 0rpx;
}

.noactive {
  filter: grayscale(80%);
  -webkit-filter: grayscale(80%);
}

/* .noactive.countcell {
  border-color: #8d775e;
}

.noactive .countheader {
  background-color: #8d775e;
} */

.red {
  color: #e60c12;
}
.yellow {
  color: #ffc000;
}
.countheader {
  font-size: 80%;
  margin: 5rpx;
  padding: 10rpx;
  color: #ffffff;
  background-color: #6a3906;
  border-radius: 0 0 20rpx 20rpx;
}

.count {
  font-size: 140%;
  font-weight: bold;
}
.counttips {
  font-size: 70%;
  font-weight: lighter;
}
.blocker {
  width: 95%;
  height: 30rpx;
  margin: 10rpx;
  margin-top: 1rpx;
}

.content {
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  text-align: center;
  padding: 50rpx 20rpx;
}
.row-container {
  /* display: flex;
    flex-direction:row;
    width:100%;
    border-bottom: 1rpx solid #eeeeee;
    vertical-align: center;
    padding:10rpx 20rpx;
    margin:0;
    background-color: #ffffff;
    text-align: left;


    z-index: 5;
    transition: left 0.2s ease-in-out;
    white-space:nowrap;
    overflow:hidden;
